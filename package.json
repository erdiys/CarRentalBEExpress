{
  "name": "nodejs",
  "packageManager": "yarn@3.6.4",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "gendoc": "node ./src/helpers/swaggerAutogen.js",
    "seed": "node ./prisma/seeder/index.js",
    "docker:up": "sudo docker compose up -d",
    "docker:down": "sudo docker compose down",
    "seed:test": "dotenv -e .env.test -- node ./prisma/seeder/index.js",
    "migrate:deploy": "yarn prisma migrate deploy",
    "pretest": "dotenv -e .env.test -- yarn migrate:deploy && yarn seed:test",
    "test": "yarn pretest && dotenv -e .env.test -- jest -i --collectCoverage "

  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "eslint": "^9.11.1",
    "globals": "^15.10.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prisma": "^5.20.0",
    "supertest": "^7.0.0"
  },
  "dependencies": {
    "@appland/appmap": "^3.171.0",
    "@prisma/client": "^5.20.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.5.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "ejs": "^3.1.10",
    "express": "^4.21.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pdfkit": "^0.15.0",
    "pg": "^8.13.0",
    "puppeteer": "^23.5.2",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.1",
    "xlsx": "^0.18.5"
  }
}
